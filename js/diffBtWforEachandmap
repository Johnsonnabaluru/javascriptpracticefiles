let arr=["john","mowa","steeph","panda","guru"]
let values=arr.forEach((x)=>{
    return x
})

console.log(values); //if we use return in forEach we will get o/p as undefined


arr.forEach((y)=>{
    console.log(y); //it will give the o/p
})

let result=arr.map((z)=>{
    return z
})

console.log(...result); //